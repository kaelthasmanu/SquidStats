HTTP_PROXY=""
HTTPS_PROXY=
NO_PROXY=
SQUID_CONFIG_PATH=/home/manuel/Desktop/config/squid.conf
ACL_FILES_DIR=/home/manuel/Desktop/config/acls
LISTEN_HOST="127.0.0.1"
LISTEN_PORT=8080
FIRST_PASSWORD="mipasswordsegura"
JWT_EXPIRY_HOURS=24
MAX_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION_MINUTES=15

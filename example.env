# Get your API credentials from https://my.telegram.org/apps
TELEGRAM_ENABLED=false
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
# Option 1: Use a bot (recommended) - Get token from @BotFather
TELEGRAM_BOT_TOKEN=
# Option 2: Use your user account (requires phone number and authentication)
TELEGRAM_PHONE=
# Session name for storing authentication
TELEGRAM_SESSION_NAME=squidstats_bot
# Comma-separated list of recipients (usernames with @, user IDs, or chat IDs)
# Example: @username1,@username2,123456789
TELEGRAM_RECIPIENTS=

